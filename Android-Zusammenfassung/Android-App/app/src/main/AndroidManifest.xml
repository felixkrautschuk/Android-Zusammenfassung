<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.felix.android_app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".StundenplanActivity"
            android:label="@string/title_activity_stundenplan" >
        </activity>
        <activity
            android:name=".NotenActivity"
            android:label="@string/title_activity_noten" >
        </activity>
        <activity
            android:name=".MensaActivity"
            android:label="@string/title_activity_mensa" >
        </activity>
    </application>

</manifest>
